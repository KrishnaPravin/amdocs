package com.amdocs.CleanCodePractices.single_entry_single_exit;

public class SingleEntrySingleExit {
	
	final static String FAIL = "Fail";
	final static String PASS = "Pass";
	final static String FIRST_CLASS = "first Class";
	final static String DISTINCTION = "Distinction";
	
	
	public static void main(String[] args) {
		System.out.print(getExamResult(50));
	}
	
	static String getExamResult(int examMark) {
		String result = "";
		if (examMark < 50) {
			result =  FAIL;
		} else {
			if (examMark < 65) {
				result = PASS;
			} else if (examMark < 85) {
				result = FIRST_CLASS;
			} else {
				result = DISTINCTION;
			}
		}
		return result;
	}

}
